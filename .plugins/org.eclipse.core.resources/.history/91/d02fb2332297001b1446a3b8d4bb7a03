package boardgame;

public class Piece {

	protected Position position;
	private Board board;
	
	public Piece() {}
	
	public Piece(Position postion, Board board) {
		this.position = postion;
		this.board = board;
	}
	

	
	public Board getBoard() {
		return board;
	}
	
	public void setBoard(Board board) {
		this.board = board;
	}
}
