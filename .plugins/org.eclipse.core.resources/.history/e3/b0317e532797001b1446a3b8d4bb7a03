package application;

import chess.ChessPiece;

public class UI {

	public static void printBoard(ChessPiece[][]pieces) {
		
	}
	private static void printPiece(ChessPiece) {
		if(piece == null) {
			System.out.println("-");
		}
		else {
			System.out.println(piece);
		}
		System.out.println(" ");
	}
}
